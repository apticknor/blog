{{> header}}

    <div class="tier">
        <div class="wrapper">

            <h1>{{ archiveYear }}</h1>

            <ul>
            {{#each collections.articles}}
                {{#checkYear ../archiveYear this.date}}
                <li>
                    <h2>{{ this.title }}</h2>
                    <div>Posted {{ formatDate this.date }}</div>
                </li>
                {{/checkYear}}
            {{/each}}
            </ul>

        </div>
    </div>

{{> footer}}