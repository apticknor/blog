{{> header}}

    <div class="tier">
        <div class="wrapper">

            {{{ contents }}}

            <ul>
            {{#each collections.articles}}
                <li>
                    <h2>{{ this.title }}</h2>
                    <div>Posted {{ formatDate this.date }}</div>
                </li>
            {{/each}}
            </ul>

        </div>
    </div>

{{> footer}}